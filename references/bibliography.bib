@article{cstar:Wiese1992,
    title = {C- and G-periodic QCD at finite temperature},
    journal = {Nuclear Physics B},
    volume = {375},
    number = {1},
    pages = {45-66},
    year = {1992},
    issn = {0550-3213},
    doi = {https://doi.org/10.1016/0550-3213(92)90333-7},
    url = {https://www.sciencedirect.com/science/article/pii/0550321392903337},
    author = {U.-J. Wiese},
    abstract = {When shifted a distance L in a spatial direction a C-periodic field is replaced by its charge conjugate. C-periodic boundary conditions break the Z(3) center symmetry of SU(3) pure gauge theory explicitly. As a consequence, the Polyakov loop, as the order parameter of the deconfinement transition to the gluon plasma phase , does not vanish in a finite C-periodic volume (while it is zero when periodic boundary conditions are used). When quarks are included one can use G-periodic boundary conditions, where G denotes G-parity. G-periodic boundary conditions break SU(2)R⊗SU(2)L⊗U(1)R=L chiral symmetry explicitly down to SU(2)R=L⊗Z(2)R=L. Theneven in the massless limit the chiral order parameter 〈ψψ〉 which determines the transition tothe quark-gluon plasma phase, does not vanish in a finite volume. The effect of C-periodic and G-periodic boundary conditions on the dynamics of the Goldstone bosons is discussed in the framework of low-energy chiral lagrangians.}
}

